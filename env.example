# Aura Finance Environment Variables
# Copy this file to .env and fill in your values

# Database Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Services Configuration

# Google Gemini API (Free Tier - 150 requests/day)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud Vertex AI (Pay-as-you-go - 10,000+ requests/day)
VITE_GCP_PROJECT_ID=your_gcp_project_id_here
VITE_GCP_API_KEY=your_gcp_api_key_here
VITE_GCP_LOCATION=us-central1

# Optional: Plaid Integration
VITE_PLAID_CLIENT_ID=your_plaid_client_id_here
VITE_PLAID_SECRET=your_plaid_secret_here
VITE_PLAID_ENV=sandbox

# Optional: OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# App Configuration
VITE_APP_NAME=Aura Finance
VITE_APP_VERSION=1.1.0
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_PLAID_INTEGRATION=false
VITE_ENABLE_OAUTH=false 