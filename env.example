# Aura Finance Environment Variables
# Copy this file to .env and fill in your values

# Database Configuration
# Supabase (for analytics and denormalized data)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Firebase Configuration (for cross-device sync - FREE)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef

# AI Services Configuration

# Google Gemini API (Free Tier - 150 requests/day)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Hugging Face Inference API (Free Tier - 500 requests/day, 5/min)
VITE_HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Optional: Plaid Integration
VITE_PLAID_CLIENT_ID=your_plaid_client_id_here
VITE_PLAID_SECRET=your_plaid_secret_here
VITE_PLAID_ENV=sandbox

# Optional: OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# App Configuration
VITE_APP_NAME=Aura Finance
VITE_APP_VERSION=1.1.0
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_PLAID_INTEGRATION=false
VITE_ENABLE_OAUTH=false 