{
  "name": "aura-finance",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 50",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "echo \"Type checking not configured - skipping\"",
    "validate": "npm run lint && npm run format:check",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "bundle:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "ci:test": "npm run lint && npm run format:check && npm run build",
    "ci:full": "npm run ci:test && npm run security:audit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "dexie": "^3.2.4",
    "lucide-react": "^0.294.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-plaid-link": "^4.0.0",
    "recharts": "^2.8.0",
    "tesseract.js": "^4.1.1",
    "uuid": "^11.1.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "postcss": "^8.4.24",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "vite": "^4.4.5"
  }
}
