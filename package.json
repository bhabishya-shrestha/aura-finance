{
  "name": "aura-finance",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "description": "Personal finance management application with mobile optimization",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 100",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:local": "npm run lint && npm run test && npm run build && npm run test:build",
    "test:unit": "vitest --run unit",
    "test:integration": "vitest --run integration",
    "test:e2e": "vitest --run e2e",
    "test:coverage": "vitest --coverage",
    "test:security": "npm audit --audit-level=high && npm run security",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "security": "node scripts/fix-security-vulnerabilities.cjs",
    "audit:fix": "npm audit fix",
    "test:build": "node scripts/test-build.cjs",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major",
    "demo:setup": "node scripts/create-demo-account.js",
    "demo:info": "echo \"Demo Account: test@gmail.com / demo123\" && echo \"Demo URL: https://aura-finance-tool.vercel.app/auth\"",
    "oauth:setup": "node scripts/setup-oauth.cjs",
    "oauth:configure": "node scripts/configure-oauth.cjs",
    "oauth:verify": "node scripts/verify-oauth.cjs",
    "deploy:staging": "vercel --prod --target staging",
    "deploy:production": "vercel --prod",
    "deploy:rollback": "vercel rollback"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "dexie": "^3.2.4",
    "lucide-react": "^0.294.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plaid-link": "^4.0.0",
    "react-router-dom": "^6.8.1",
    "recharts": "^2.8.0",
    "uuid": "^11.1.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "audit-ci": "^7.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.24",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  }
}
